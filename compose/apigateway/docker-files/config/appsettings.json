{
  "Database":"sqlite",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=db/apigateway.db"
  },


  "LogSettings": {
    "UseElasticSearch": true,
    "ElasticSearchUrl": "http://esforlog:9200",
    "LogEventLevel": 2, //0: Verbose, 1:Debug,, 2:Information, 3:Warning, 4:Error, 5:Fatal
    "IndexNamePrefix": "apigateway"
  },

  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}

